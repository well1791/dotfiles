[
  // space binding
  {
    "context": "Editor && (vim_mode == normal || vim_mode == helix_normal) && vim_operator == none && !VimWaiting",
    "bindings": {
      "space e": "workspace::ToggleLeftDock"
    }
  },
  {
    "context": "ProjectPanel",
    "bindings": {
      "escape": "workspace::ToggleLeftDock",
      "space e": "workspace::ToggleLeftDock"
    }
  }
]
